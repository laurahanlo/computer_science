def is_friend(name):
	if name[0] == 'D'
		return true
	else :
		return false

print is_friend('Diane')
print is_friend('Fred')

		OR	

def is_friend(name):
	return name[0] == 'D'

print is_friend('Diane')
print is_friend('Fred')